<h1>Listing consult_replies</h1>

<table>
  <thead>
    <tr>
      <th>Consult subject</th>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consult_replies.each do |consult_reply| %>
      <tr>
        <td><%= consult_reply.consult_subject %></td>
        <td><%= consult_reply.content %></td>
        <td><%= consult_reply.user %></td>
        <td><%= link_to 'Show', consult_reply %></td>
        <td><%= link_to 'Edit', edit_consult_reply_path(consult_reply) %></td>
        <td><%= link_to 'Destroy', consult_reply, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consult reply', new_consult_reply_path %>
