<h1>Listing messages</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Msg type</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @messages.each do |message| %>
      <tr>
        <td></td>
        <td><%= message.msg_type %></td>
        <td><%= message.user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
